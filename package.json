{
  "name": "ogma",
  "version": "1.0.0",
  "description": "API for language",
  "main": "dist/index.js",
  "author": "Matthieu Rondeau <mr@umanlife.com>",
  "license": "MIT",
  "homepage": "https://github.com/milanito/ogma",
  "config": {
    "dockerimage": "mrumanlife/ogma"
  },
  "scripts": {
    "build": "gulp build",
    "start": "node dist",
    "watch": "gulp watch",
    "test": "node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=test DB_HOST=mongodb://localhost/translation-test nyc --reporter=lcovonly mocha --compilers js:babel-register",
    "docker-build": "docker build -t $npm_package_config_dockerimage .",
    "create-admin": "node_modules/babel-cli/bin/babel-node.js -- scripts/create-admin.script.js",
    "doc": "./node_modules/.bin/esdoc",
    "lint": "./node_modules/.bin/eslint src",
    "prepush": "node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=test DB_HOST=mongodb://localhost/translation-test nyc --reporter=lcovonly mocha --compilers js:babel-register && ./node_modules/.bin/eslint src"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "boom": "^5.2.0",
    "brok": "^1.1.1",
    "bunyan": "^1.8.12",
    "country-language": "^0.1.7",
    "hapi": "^16.5.2",
    "hapi-auth-jwt2": "^7.3.0",
    "hapi-bunyan": "^0.7.0",
    "hapi-cors": "^1.0.1",
    "hapi-rbac": "^2.3.0",
    "http-status": "^1.0.1",
    "inert": "^4.2.1",
    "jsonwebtoken": "^7.4.2",
    "lodash": "^4.17.4",
    "lout": "^10.0.2",
    "mongoose": "^4.11.7",
    "vision": "^4.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.1",
    "cross-env": "^5.0.5",
    "esdoc": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.4.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-changed": "^3.1.0",
    "gulp-eslint": "^4.0.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sequence": "^0.4.6",
    "husky": "^0.14.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "supertest": "^3.0.0"
  }
